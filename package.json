{
	"name": "@cfware/history-state",
	"version": "0.1.0",
	"description": "Browser History API state manager",
	"main": "history-state.js",
	"scripts": {
		"release": "standard-version --sign",
		"test": "xo && nyc ava -sv"
	},
	"engines": {
		"node": ">=8"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"files": [
		"*.js",
		"!babel.config.js"
	],
	"keywords": [
		"spa",
		"history-api",
		"browser"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/history-state.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/history-state/issues"
	},
	"homepage": "https://github.com/cfware/history-state#readme",
	"devDependencies": {
		"@babel/plugin-transform-modules-commonjs": "^7.2.0",
		"@babel/register": "^7.0.0",
		"@cfware/ava-selenium-manager": "^0.2.0",
		"@cfware/eslint-config-browser": "^0.2.0",
		"@cfware/fastify-test-helper": "^0.2.1",
		"@istanbuljs/nyc-config-babel": "^2.0.1",
		"ava": "^1.3.1",
		"babel-plugin-istanbul": "^5.1.1",
		"nyc": "^13.3.0",
		"standard-version": "^5.0.2",
		"xo": "^0.24.0"
	},
	"nyc": {
		"all": true,
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"extends": "@istanbuljs/nyc-config-babel"
	},
	"xo": {
		"overrides": [
			{
				"files": [
					"*.js",
					"!babel.config.js"
				],
				"extends": "@cfware/eslint-config-browser"
			}
		]
	}
}
