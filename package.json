{
	"name": "@cfware/history-state",
	"version": "0.3.0",
	"description": "Browser History API state manager",
	"main": "history-state.js",
	"scripts": {
		"release": "standard-version --sign",
		"test": "xo && nyc ava -sv"
	},
	"engines": {
		"node": ">=8"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"files": [
		"*.js",
		"!nyc.config.js"
	],
	"keywords": [
		"spa",
		"history-api",
		"browser"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/history-state.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/history-state/issues"
	},
	"homepage": "https://github.com/cfware/history-state#readme",
	"devDependencies": {
		"@cfware/ava-selenium-manager": "^0.2.1",
		"@cfware/eslint-config-browser": "^0.2.1",
		"@cfware/fastify-test-helper": "^0.2.3",
		"@cfware/nyc": "^0.4.2",
		"ava": "^1.4.1",
		"chromedriver": "^74.0.0",
		"esm": "^3.2.22",
		"geckodriver": "^1.16.2",
		"nyc": "^14.1.0",
		"standard-version": "^6.0.1",
		"xo": "^0.24.0"
	},
	"xo": {
		"overrides": [
			{
				"files": [
					"*.js",
					"!nyc.config.js"
				],
				"extends": "@cfware/eslint-config-browser"
			}
		]
	}
}
